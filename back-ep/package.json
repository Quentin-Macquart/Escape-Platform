{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.ts",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "lint": "eslint \"./**/*.js\" --fix .",
    "prettify": "prettier --write .",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ]
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "mysql2": "^2.3.3",
    "npm": "^8.4.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "@types/pg": "^8.6.5",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.16",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --ext .js --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
